{
  "name": "react-burger",
  "version": "0.1.0",
  "homepage": "./",
  "private": true,
  "dependencies": {
    "@ya.praktikum/react-developer-burger-ui-components": "^1.13.1",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-intersection-observer": "^9.4.0",
    "react-redux": "^8.0.4",
    "react-router-dom": "5.3.3",
    "redux": "^4.2.0",
    "redux-thunk": "^2.4.1",
    "uuid": "^9.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "yarn build",
    "deploy": "gh-pages -d build",
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"src/**/*.scss\" --custom-syntax 'postcss-scss'",
    "stylelint:fix": "stylelint \"src/**/*.scss\" --custom-syntax 'postcss-scss' --fix",
    "lint": "yarn editorconfig && yarn stylelint:fix",
    "prepare": "husky install",
    "staged": "lint-staged",
    "ci": "rm -rf node_modules && yarn install --frozen-lockfile"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "editorconfig-cli": [
    "*.json",
    "src/**/*.{js,jsx,ts,tsx,scss,css}"
  ],
  "lint-staged": {
    "*": "yarn lint"
  },
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "^1.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.59",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^9.0.0",
    "eslint-config-react-app": "^7.0.1",
    "gh-pages": "^4.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "postcss-scss": "^4.0.4",
    "prettier": "^2.7.1",
    "react-scripts": "5.0.1",
    "redux-devtools-extension": "^2.13.9",
    "sass": "^1.54.9",
    "stylelint": "^14.11.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.8.3"
  }
}
